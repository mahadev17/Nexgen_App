<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="@drawable/bg2"
    android:orientation="vertical"
    android:paddingTop="1dip" >
    
    

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

    
        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="21dp"
            android:layout_marginTop="130dp"
            android:layout_centerHorizontal="true"
            android:contentDescription="@string/blank"
            android:src="@drawable/registration_text" />
        
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="170dp"
            android:paddingLeft="12dip"
            android:text="@string/contact"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@android:color/white"
            android:textSize="12sp" />

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="175dip" 
            android:layout_marginBottom="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="15dp"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="8dp"
                        android:gravity="top"
                        android:paddingLeft="12dip"
                        android:text="NAME"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="4dp"
                     
                        android:paddingLeft="12dip"
                        android:text="@string/distributor"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="11dp"
                        android:layout_marginTop="4dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/number"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="11dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:layout_marginTop="8dp"
                        android:text="@string/address"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView534656"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="8dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/telephone"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView534656"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="8dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="AGE"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView5546546"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/email"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView5743556"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="14dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/password"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView5743556"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="14dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="HEARD ABOUT FESTIVAL FROM "
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView574674"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="7dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/film_information"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                   
                     <TextView
                        android:id="@+id/textView54564"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="11dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="SCREENING"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />
                    
                    
                    
                    
                    <TextView
                        android:id="@+id/textView54564"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="11dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/title"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView55876"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="11dp"
                        android:layout_marginTop="7dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/original"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView55876"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="11dp"
                        android:layout_marginTop="7dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/film_decription"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView55435"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="11dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/genre"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/textView5758"
                        android:layout_width="wrap_content"
                        android:layout_height="21dp"
                        android:layout_marginBottom="11dp"
                        android:gravity="center"
                        android:paddingLeft="12dip"
                        android:text="@string/running"
                        android:textColor="@android:color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="15dp"
                    android:orientation="vertical" >

                    
                    
                    <EditText
                        android:id="@+id/edit_name"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:ems="50"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>
                    
                    
                    <EditText
                        android:id="@+id/edit_distributor"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:ems="50"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_number"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="number"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_address"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_telephone"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="phone"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_age"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="phone"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_email_reg"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="textEmailAddress"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_password_reg"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="textPassword"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_about_me"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:textSize="12sp" >
                    </EditText>

                 
                       <EditText
                        android:id="@+id/edit_screening"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="42dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>
                    
                    
                    
                    <EditText
                        android:id="@+id/edit_film_title"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_film_original"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_description"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_genre"
                        android:layout_width="match_parent"
                        android:layout_height="30dip"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>

                    <EditText
                        android:id="@+id/edit_running"
                        android:layout_width="95dp"
                        android:layout_height="30dip"
                        android:layout_gravity="right"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1.8"
                        android:background="@drawable/textbox_3"
                        android:inputType="text"
                        android:textSize="12sp" >
                    </EditText>

                    <Button
                        android:id="@+id/btn_next_reg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="3dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/arrow_right" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>

    </RelativeLayout>

</LinearLayout>
